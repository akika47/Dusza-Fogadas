<UserControl x:Class="WPF_Dusza.CustomControls.GameRowItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_Dusza.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBox x:Name="tb_GameName"/>
        <TextBox x:Name="tb_OrganizerName"/>
        <TextBox x:Name="tb_Participants"/>
        <TextBox x:Name="tb_Events"/>
        <Button x:Name="btn_NewRowItem"  Click="Handle_Click"/>
    </Grid>
</UserControl>
